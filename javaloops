import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
        Scanner scn = new Scanner(System.in);
        System.out.println("Enter the first number:");
        int n = scn.nextInt();
        for(int i = 0 ; i < 10 ; i++)
        {
            
            
            System.out.println(n + " x " + (i+1) + " = " + (n * (i+1)));
        }
       
        
      

      
    }
}
